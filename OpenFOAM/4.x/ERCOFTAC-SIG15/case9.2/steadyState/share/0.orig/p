FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            cyclic;
    }           
    
    outlet
    {
        type            cyclic;
    }

    empty1       
    {
        type            empty;
    }

    empty2
    {
        type            empty;
    }
 
    bottom       
    {
        type            zeroGradient;
    }

    top 
    {
        type            zeroGradient;
    }
}

